create table chi_tiet_don_hang (
	id_don_hang varchar(255) primary key not null,
    id_dat_hang varchar(255),
    id_thuoc varchar(255),
    so_luong int not null,
    don_gia decimal not null,
    thanh_tien decimal generated always as (so_luong * don_gia) stored,
    foreign key (id_dat_hang) references dat_hang (id_dat_hang),
    foreign key (id_thuoc) references thuoc (id_thuoc)
);
insert into chi_tiet_don_hang (id_don_hang, id_dat_hang, id_thuoc, so_luong, don_gia) values  
