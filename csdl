create table van_chuyen (
	id_van_chuyen varchar(255) primary key not null,
    id_dat_hang varchar(255),
    trang_thai_van_chuyen nvarchar(255) default 'Chưa hoàn thành' not null,
    phuong_thuc_van_chuyen nvarchar(255) default 'Tiêu chuẩn' not null,
    ngay_giao_hang date,
    ngay_van_chuyen date,
    foreign key (id_dat_hang) references dat_hang (id_dat_hang)
);
insert into van_chuyen (id_van_chuyen, id_dat_hang, trang_thai_van_chuyen, phuong_thuc_van_chuyen, ngay_giao_hang, ngay_van_chuyen) values
