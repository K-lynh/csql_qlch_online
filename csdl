create table danh_gia (
	id_danh_gia varchar(255) primary key not null,
    id_khach_hang varchar(255),
    id_thuoc varchar(255),
    diem_xep_hang int default '5',
    binh_luan text,
    ngay_danh_gia date,
    foreign key (id_khach_hang) references khach_hang (id_khach_hang),
    foreign key (id_thuoc) references thuoc (id_thuoc)
);
insert into danh_gia (id_danh_gia, id_khach_hang, id_thuoc, diem_xep_hang, binh_luan, ngay_danh_gia) values
