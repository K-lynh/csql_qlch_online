use csdl_qlch_online;
create table nguoi_dung (
	id_nguoi_dung varchar(255) primary key not null,
    vai_tro nvarchar(255) default 'Khách hàng' not null,
    ten_nguoi_dung nvarchar(255) not null,
    so_dien_thoai char(45) not null,
    email varchar(255) not null, 
    mat_khau varchar(255) not null,
    dia_chi nvarchar(255) not null
);
