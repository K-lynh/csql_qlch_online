create table quan_ly_cua_hang (
	id_quan_ly_cua_hang varchar(255) primary key not null,
    ngay_bat_dau date not null,
    chuc_vu nvarchar(255) not null,
    id_nguoi_dung varchar(255),
    foreign key (id_nguoi_dung) references nguoi_dung (id_nguoi_dung)
);
insert into quan_ly_cua_hang (id_quan_ly_cua_hang, ngay_bat_dau, chuc_vu, id_nguoi_dung) values
