create table thanh_toan (
	id_thanh_toan varchar(255) primary key not null,
    id_dat_hang varchar(255),
    trang_thai_thanh_toan nvarchar(255) default 'Chưa thanh toán' not null,
    so_tien decimal not null,
    phuong_thuc_thanh_toan nvarchar(255) default 'Thanh toán khi nhận hàng' not null,
    foreign key (id_dat_hang) references dat_hang (id_dat_hang)
);
insert into thanh_toan (id_thanh_toan, id_dat_hang, trang_thai_thanh_toan, so_tien, phuong_thuc_thanh_toan) values
