create table dat_hang (
	id_dat_hang varchar(255) primary key not null,
    id_khach_hang varchar(255),
    ngay_tao date not null,
    trang_thai nvarchar(255) not null,
    tong_tien decimal,
    foreign key (id_khach_hang) references khach_hang (id_khach_hang)
);
insert into dat_hang (id_dat_hang, id_khach_hang, ngay_tao, trang_thai, tong_tien) values
